<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.huynh.jess.jhuynh_habittracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HabitTrackerMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".HabitTrackerCreatorActivity"></activity>
        <activity android:name=".HabitTrackerSingleActivity"></activity>
        <activity android:name=".HabitTrackerCompletedActivity"></activity>

    </application>

</manifest>